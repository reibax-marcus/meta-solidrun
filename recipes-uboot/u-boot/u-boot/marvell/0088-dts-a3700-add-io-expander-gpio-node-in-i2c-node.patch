From 23f054d9dc62ba63e9d446df046d07bb145a4394 Mon Sep 17 00:00:00 2001
From: zachary <zhangzg@marvell.com>
Date: Fri, 17 Mar 2017 10:47:57 +0800
Subject: [PATCH] dts: a3700: add io expander gpio node in i2c node

Change-Id: I5c2660e013da72bdf193884d83d203a75d2356b1
Signed-off-by: zachary <zhangzg@marvell.com>

---
 arch/arm/dts/armada-3720-db.dts | 9 +++++++++
 1 file changed, 9 insertions(+)

diff --git a/arch/arm/dts/armada-3720-db.dts b/arch/arm/dts/armada-3720-db.dts
index 8882f23b41..4543c66af8 100644
--- a/arch/arm/dts/armada-3720-db.dts
+++ b/arch/arm/dts/armada-3720-db.dts
@@ -91,6 +91,15 @@
 	pinctrl-names = "default";
 	pinctrl-0 = <&i2c1_pins>;
 	status = "okay";
+
+	#address-cells = <1>;
+	#size-cells = <0>;
+	expander0: pca953x@22 {
+		compatible = "nxp,pca9555";
+                #gpio-cells = <2>;
+		reg = <0x22>;
+		status = "okay";
+	};
 };
 
 /* CON3 */
